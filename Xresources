! -- Variable font size
URxvt.keysym.C-equal:			perl:font-size:increase
URxvt.keysym.C-minus:			perl:font-size:decrease
!URxvt.keysym.C-0: command:\033]710;xft:inconsolata:size=10\007
!URxvt.keysym.C-minus: command:\033]710;xft:inconsolata:size=8\007
!URxvt.keysym.C-equal: command:\033]710;xft:inconsolata:size=12\007

